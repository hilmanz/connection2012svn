/* ============ TOOLTIP =================*/
.tip {
    color: #fff;
    background:#222;
    display:none; /*--Hides by default--*/
    padding:10px;
	position:absolute;
    z-index:1000;
    font-size:11px;
    text-shadow:none;
	top:-90px;
	width:100px;
	left:30px;
	text-align:center;
}
.tip p{
	margin:0 0 5px 0;
}
.tip img{
	background:#fff;
}
.tip .arrow {
    display:block;
    position:absolute;
    background:url(../img/arrow_tip.png) no-repeat;
    width:15px;
    height:13px;
    margin:0 0 -11px -8px;
    left:8px;
	bottom:0;
}
.tip_trigger{
	position:relative;
}

.tip-yellow {
	z-index:1000;
	text-align:left;
	padding:8px;
	min-width:50px;
	max-width:180px;
	font-size:11px;
	color:#fff;
	background:#222;
}
.tip-yellow .tip-inner {
	margin-top:-2px;
	padding:0 3px 1px 3px;
}

/* Configure an arrow image - the script will automatically position it on the correct side of the tip */
.tip-yellow .tip-arrow-top {
	margin-top:-7px;
	margin-left:15px;
	top:0;
	left:0;
	width:16px;
	height:10px;
    background:url(../img/arrow_tip.png) no-repeat;
}
.tip-yellow .tip-arrow-right {
	margin-top:-9px; /* approx. half the height to center it */
	margin-left:-4px;
	top:50%;
	left:100%;
	width:10px;
	height:20px;
    background:url(../img/arrow_tip.png) no-repeat;
}
.tip-yellow .tip-arrow-bottom {
	margin-top:-6px;
	margin-left:15px;
	top:100%;
	left:0;
	width:16px;
	height:13px;
    background:url(../img/arrow_tip.png) no-repeat;
}
.tip-yellow .tip-arrow-left {
	margin-top:-9px; /* approx. half the height to center it */
	margin-left:-6px;
	top:50%;
	left:0;
	width:10px;
	height:20px;
    background:url(../img/arrow_tip.png) no-repeat;
}



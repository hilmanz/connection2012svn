/* ============ NAVIGATION =================*/
#navigation{
	text-transform:uppercase;
	position:absolute;
	width:700px;
	top:20px;
	left:300px;
	letter-spacing:-0.06em;
}
#navigation ul{
	list-style:none;
	margin:0;
	padding:0;
}
#navigation ul li{
	float:left;
	margin:0 25px 0 0;
}
#navigation ul li.last{
	margin:0;
}
#navigation ul ul li{
	letter-spacing:-0.05em;
	float:none;
	margin:0;
}
#navigation a{
	color:#000;
	font-size:18px;
	padding:0 0 8px 0;
	margin:0 0 5px 0;
	display:block;
	text-decoration:none;
}
#navigation a.current{
	background:url(../img/line.png) no-repeat bottom;
	color:#666;
}
#navigation ul ul li a.current{
	background:url(../img/arrow_menu.png) no-repeat  -5px 2px ;
	color:#000;
}
#navigation ul ul li a{
	font-size:12px;
	padding:0 0 0 15px;
	margin:0;
	color:#666;
}
#subnavigation{
	padding:30px 10px 0 0;
	clear:both;
	position:absolute;
	z-index:2;
	margin:0 0 0 105px;
	width:800px;
	right:0;
text-align:right;
}
#subnavigation a{
	font-size:13px;
	color:#333;
	text-decoration:none;
	text-transform:uppercase;
	padding:0 10px;
}
#navigation a:hover,#navigation ul ul li a:hover,#subnavigation a:hover,#subnavigation a.current{
	color:#C00;
}
